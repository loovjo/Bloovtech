package com.loovjo.bloovtech.tileentity;

import net.minecraft.tileentity.TileEntity;

public class TileEntityEnergyTransferer extends TileEntity {
	private int x, y, z;
	private boolean hasChecked;
	
	public void updateEntity() {
		if (!hasChecked) {
			hasChecked = true;
			x = xCoord;
			y = yCoord;
			z = zCoord;
		}
	}
}
